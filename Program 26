#include <stdio.h>

int main() {
    int a, b, product;
    
    printf("Enter two integers: ");
    scanf("%d %d", &a, &b);

    product = a * b;
    printf("Product = %d\n", product);

    if(product % 2 == 0)
        printf("Product is Even\n");
    else
        printf("Product is Odd\n");

    return 0;
}
