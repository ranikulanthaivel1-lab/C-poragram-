#include <stdio.h>

int main() {
    int age;

    printf("Enter age: ");
    scanf("%d", &age);

    if (age <= 5)
        printf("Mashed vegetables");
    else if (age <= 18)
        printf("Burgers / Pasta");
    else if (age <= 50)
        printf("Balanced diet");
    else
        printf("Soup / Fish");

    return 0;
}
